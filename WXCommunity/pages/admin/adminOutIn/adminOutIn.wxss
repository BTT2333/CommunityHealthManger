page {
  background: #efeff4;
  color: #38383d;
  font-size: 32rpx;
}

.page-section {
  width: 100%;
  margin-top: 0rpx;
  padding-bottom: 30rpx;
}

.my_flex {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.rich-text-wrp {
  padding-left: 32rpx;
  border-bottom: 1px solid #f2f2f2;
  background-color: #fff;
  line-height: 80rpx;
}

.run {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;

}

.my_flexR {
  position: absolute;
  right: 48rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 230px;
  min-width: 200rpx;
  text-align: right;
  overflow: hidden;
}

.my_flexImg {
  position: absolute;
  right: 16rpx;
}


.data-item{
  margin-bottom: 30rpx;
  background-color: white;
}
.data-item-out {
  padding: 20rpx;
}
.data-item-type {
  margin-left: 5rpx;
  border-bottom: 1px solid #eee;
}
.data-item-que {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 700rpx;
  height: 200rpx;
  border-bottom: 1px solid #eee;
}
.data-item-img {
  width: 210rpx;
  height: 140rpx;
  margin-right: 20rpx;
}
.data-item-long{
  border-bottom: 1px solid #eee;
}
.data-item-phone{
  border-bottom: 1px solid #eee;
}

.data-item-status{
  margin-top: 20rpx;
  width: 130rpx;
  height: 60rpx;
  font-weight: 550;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
}
.done{
  color: #3586c4;
  border: 1px solid #3586c4;
}
.not{
  color: #65c2a9;
  border: 1px solid #65c2a9;
}