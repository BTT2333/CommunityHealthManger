.searchbar-result {
  margin-top: 0;
  font-size: 14px
}

.searchbar-result .weui-cell__bd {
  padding: 2px 0 2px 20px;
  color: var(--weui-FG-1)
}

.searchbar-result:before {
  display: none
}

.data-item-out {
  margin: 20rpx;
}
.data-item-type {
  margin-left: 5rpx;
  border-bottom: 1px solid #eee;
}
.data-item-que {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 700rpx;
  height: 200rpx;
  border-bottom: 1px solid #eee;
}
.data-item-img {
  width: 210rpx;
  height: 140rpx;
  margin-right: 20rpx;
}
.data-item-question{
  border-bottom: 1px solid #eee;
}
.data-item-phone{
  border-bottom: 1px solid #eee;
}
.data-item-hospital{
  margin-top: 20rpx;
  width: 270rpx;
  height: 60rpx;
  font-weight: 550;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  color: #65c2a9;
  border: 1px solid #65c2a9;
}

.middle{
  height: 30rpx;
  background-color: rgb(244, 242, 242);
}